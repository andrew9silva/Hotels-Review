<%= form_for @hotel do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :location %>
    <%= f.text_field :location, placeholder: "City, State" %>

    <%= f.label :price_level %>
    <%= f.text_field :price_level, placeholder: "1-5" %>

    <%= f.label :amenities %>
    <%= f.text_field :amenities %>

    <%= f.hidden_field :user_id %>

    <%= f.submit %>

<% end %>