<h1>Sign Up!</h1>

<%= form_for(@user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit 'Sign Up' %>
<% end %><br><br>
<%= link_to('Log in with Google!', '/auth/google_oauth2', method: :post) %>